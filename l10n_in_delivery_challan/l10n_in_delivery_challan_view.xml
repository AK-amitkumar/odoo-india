<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    	<record model="ir.ui.view" id="res_company_form_402_report">
	        <field name="name">res.company.form.402</field>
	        <field name="inherit_id" ref="l10n_in_tax_retail_invoice.res_company_tax_invoice_report"/>
	        <field name="model">res.company</field>
	        <field name="type">form</field>
	        <field name="arch" type="xml">
	            <xpath expr="//notebook/page[@string='Tax Details']" position="after">
	                <page string="Challan Configuration">
	                	<group>
	                		<group>
                				<field name="processing_duration"/>
	                		</group>
	                	</group>
                		<field name="challan_purpose" placeholder="Challan Purpose"/>
	                </page>
	            </xpath>
       		</field>
    	</record>
    	
    </data>
</openerp>