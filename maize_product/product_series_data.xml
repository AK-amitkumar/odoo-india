<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    
        <record id="product_cm" model="product.order.series">
            <field name="code">CM</field>
            <field name="name">CM</field>
            <field name="type">purchase</field>
        </record>


        <record id="product_ch" model="product.order.series">
            <field name="code">CH</field>
            <field name="name">Chemical</field>
            <field name="type">purchase</field>
        </record>

        <record id="product_pk" model="product.order.series">
            <field name="code">PK</field>
            <field name="name">Packing</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_st" model="product.order.series">
            <field name="code">ST</field>
            <field name="name">Store</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_cp" model="product.order.series">
            <field name="code">CP</field>
            <field name="name">Capital</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_co" model="product.order.series">
            <field name="code">CO</field>
            <field name="name">Contract</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_lb" model="product.order.series">
            <field name="code">LB</field>
            <field name="name">Laboratory</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_rp" model="product.order.series">
            <field name="code">RP</field>
            <field name="name">Repair</field>
            <field name="type">purchase</field>
        </record>

        <record id="product_ms" model="product.order.series">
            <field name="code">MS</field>
            <field name="name">Miscelaneous</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_fa" model="product.order.series">
            <field name="code">FA</field>
            <field name="name">Fly Ash Bricks</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_fc" model="product.order.series">
            <field name="code">FC</field>
            <field name="name">Filter Cloth</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_ft" model="product.order.series">
            <field name="code">FT</field>
            <field name="name">Free Trial</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_im" model="product.order.series">
            <field name="code">IM</field>
            <field name="name">Imported</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_wo" model="product.order.series">
            <field name="code">WO</field>
            <field name="name">Work Order</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_sp" model="product.order.series">
            <field name="code">SP</field>
            <field name="name">Special Purchase</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_og" model="product.order.series">
            <field name="code">OG</field>
            <field name="name">Oil / Grease</field>
            <field name="type">purchase</field>
        </record>
        <record id="product_mm" model="product.order.series">
            <field name="code">MM</field>
            <field name="name">Mininmum / Maximum</field>
            <field name="type">purchase</field>
        </record>


        <!-- UOM Data -->
        <record id="product.product_uom_no" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_unit"/>
            <field name="name">NO</field>
            <field name="factor" eval="1"/>
        </record>
        <record id="product.product_uom_kgm" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_kgm"/>
            <field name="name">KG</field>
            <field name="factor" eval="1"/>
        </record>
        <record id="product.product_uom_litre" model="product.uom">
            <field name="name">LT</field>
            <field name="category_id" ref="product.product_uom_categ_vol"/>
            <field name="factor">1.0</field>
        </record>
        <record id="product.product_mt" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_kgm"/>
            <field name="name">MT</field>
            <field name="factor_inv" eval="1000"/>
            <field name="uom_type">bigger</field>
        </record>
        <record id="product_uom_mr" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">MR</field>
            <field name="factor" eval="1.0"/>
        </record>
        <record id="product_uom_mm" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">MM</field>
            <field name="factor" eval="1000"/>
            <field name="uom_type">smaller</field>
        </record>
        <record id="product_uom_sm" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">SM</field>
            <field name="factor" eval="1.0"/>
        </record>
        <record id="product_uom_sf" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">SF</field>
            <field name="factor" eval="1.0"/>
        </record>
        <record id="product_uom_si" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">SI</field>
            <field name="factor" eval="144.0"/>
            <field name="uom_type">smaller</field>
        </record>
        <record id="product_uom_cd" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">CD</field>
            <field name="factor" eval="1.0"/>
        </record>
        <record id="product_uom_m3" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_vol"/>
            <field name="name">M3</field>
            <field name="factor_inv" eval="1000"/>
            <field name="uom_type">bigger</field>
        </record>
        <record id="product_uom_sc" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">SC</field>
            <field name="factor" eval="10000.0"/>
            <field name="uom_type">smaller</field>
        </record>
        <record id="product_uom_cf" model="product.uom">
            <field name="category_id" ref="product.product_uom_categ_vol"/>
            <field name="name">CF</field>
            <field name="factor_inv" eval="28.317"/>
            <field name="uom_type">bigger</field>
        </record>
        <record id="product_uom_ft" model="product.uom">
            <field name="category_id" ref="product.uom_categ_length"/>
            <field name="name">FT</field>
            <field name="factor_inv" eval="3"/>
            <field name="uom_type">bigger</field>
        </record>
        <!-- product Category data-->
        <record id="product.product_category_all" model="product.category">
            <field name="name">Local</field>
        </record>
        <record id="product.product_category_1" model="product.category">
            <field name="name">Imported</field>
        </record>
    </data>
</openerp>
