<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    	<record id="view_department_excise_percentage" model="ir.ui.view">
            <field eval="1" name="priority"/>
            <field name="name">department.excise.percentage.form</field>
            <field name="model">department.excise.percentage</field>
            <field name="arch" type="xml">
                <form string="Details" version="7.0">
	                <group>
	                	<field name="department_id"/>
	                	<field name="excise_percentage"/>
	                </group>
                    <footer>
                    	<button name="get_data" string="OK" type="object" class="oe_highlight"/> or
                    	<button special="cancel" string="Close" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>
        
        <record id="action_department_excise_percentage" model="ir.actions.act_window">
            <field name="name">Inputs Consumed by Department</field>
            <field name="res_model">department.excise.percentage</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_department_excise_percentage"/>
            <field name="target">new</field>
        </record>
        
    </data>
</openerp>