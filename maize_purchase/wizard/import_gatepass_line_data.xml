<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      <record id="view_import_gatepass_form_line" model="ir.ui.view">
        <field name="name">import_inward_data__lineform</field>
        <field name="model">import.gatepass.line.data</field>
        <field name="arch" type="xml">
            <form string="Import GATEPASS LINE" version="7.0">
               <group>
                    <separator string="Import Gate Pass Line Data"/>
               </group>
               <footer>
                    <button name="do_import_gatepass_data" string="Import" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
               </footer>
           </form>
        </field>
    </record>
    <act_window 
        name="Import GatePass Lines"
        res_model="import.gatepass.line.data"
        src_model="stock.picking.out"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_gatepass_line_import" />

    </data>
</openerp>
