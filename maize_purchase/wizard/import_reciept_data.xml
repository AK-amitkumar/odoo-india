<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      <record id="view_import_receipt_data_form" model="ir.ui.view">
        <field name="name">import_receipt_data_form</field>
        <field name="model">import.receipt.data</field>
        <field name="arch" type="xml">
            <form string="Import Reciept" version="7.0">
               <group>
                    <separator string="Import Reciept Data"/>
               </group>
               <footer>
                    <button name="import_receipt_data" string="Import" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
               </footer>
           </form>
        </field>
    </record>

    <act_window 
        name="Import Reciept"
        res_model="import.receipt.data"
        src_model="stock.picking.receipt"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_import_receipt_data_form" />

    </data>
</openerp>
