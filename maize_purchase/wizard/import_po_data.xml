<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      <record id="view_import_po_form" model="ir.ui.view">
        <field name="name">import_po_data_form</field>
        <field name="model">import.po.data</field>
        <field name="arch" type="xml">
            <form string="Import po" version="7.0">
               <group>
                    <separator string="Import PO Data"/>
               </group>
               <footer>
                    <button name="do_import_po_data" string="Import" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
               </footer>
           </form>
        </field>
    </record>
    <act_window 
        name="Import PO"
        res_model="import.po.data"
        src_model="purchase.order"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_po_import" />
        
      


    </data>
</openerp>
