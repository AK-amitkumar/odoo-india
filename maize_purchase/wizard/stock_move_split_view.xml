<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <record id="maize_inherit_view_split_in_lots" model="ir.ui.view">
            <field name="name">maize.inherit.stock.split.form</field>
            <field name="model">stock.move.split</field>
            <field name="inherit_id" ref="stock.view_split_in_lots"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='location_id']" position="before">
                    <label for="challan_qty"/>
                    <div>
                        <field name="challan_qty" readonly="1" class="oe_inline"/>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</openerp>