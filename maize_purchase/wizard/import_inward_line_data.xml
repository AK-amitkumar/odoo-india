<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      <record id="view_import_inward_form_line" model="ir.ui.view">
        <field name="name">import_inward_data__lineform</field>
        <field name="model">import.inward.line.data</field>
        <field name="arch" type="xml">
            <form string="Import Inward" version="7.0">
               <group>
                    <separator string="Import Inward Line Data"/>
               </group>
               <group>
                   <field name='file_path'/>
               </group>               
               <footer>
                    <button name="do_import_inward_data" string="Import" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
               </footer>
           </form>
        </field>
    </record>
    <act_window 
        name="Import Inward Lines"
        res_model="import.inward.line.data"
        src_model="stock.picking.in"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_inword_line_import" />

    </data>
</openerp>
