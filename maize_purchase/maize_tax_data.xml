<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="maize_no_excise" model="account.tax">
            <field name="name"> No Excise</field>
            <field name="tax_type">ex</field>
            <field name="amount">0.0</field>
        </record>

        <record id="maize_excise_include" model="account.tax">
            <field name="name"> Excise Duty paid</field>
            <field name="tax_type">ex</field>
            <field name="price_include">True</field>
            <field name="amount">0.0</field>
        </record>
        <record id="maize_child_tax2" model="account.tax">
            <field name="name"> Excise 2%</field>
            <field name="tax_type">other</field>
            <field name="amount">0.02</field>
        </record>
        <record id="maize_child_tax1" model="account.tax">
            <field name="name"> Excise 1%</field>
            <field name="tax_type">other</field>
            <field name="amount">0.01</field>
        </record>
        <record id="maize_excise_12" model="account.tax">
            <field name="name"> Excise 12% (2% + 1%)</field>
            <field name="tax_type">ex</field>
            <field name="amount">0.12</field>
            <field name="include_base_amount">True</field>
            <field name="child_ids" eval="[(6,0,[ref('maize_child_tax2'), ref('maize_child_tax1')])]"/>
        </record>
        <record id="maize_vat_include" model="account.tax">
            <field name="name"> Tax paid</field>
            <field name="tax_type">other</field>
            <field name="price_include">True</field>
            <field name="amount">0.0</field>
        </record>
        <record id="maize_vat_child_12" model="account.tax">
            <field name="name"> VAT 2.5%</field>
            <field name="tax_type">other</field>
            <field name="amount">0.025</field>
        </record>
        <record id="maize_vat_child_15" model="account.tax">
            <field name="name"> VAT-2.5%</field>
            <field name="tax_type">other</field>
            <field name="amount">0.025</field>
        </record>
        <record id="maize_vat_c_form" model="account.tax">
            <field name="name"> 'C' form to be Issued</field>
            <field name="tax_type">vat</field>
            <field name="amount">0.00</field>
        </record>
        <record id="maize_vat_h_form" model="account.tax">
            <field name="name"> 'H' form to be Issued</field>
            <field name="tax_type">vat</field>
            <field name="amount">0.00</field>
        </record>
        <record id="maize_vat_4" model="account.tax">
            <field name="name"> VAT 4%</field>
            <field name="tax_type">other</field>
            <field name="amount">0.04</field>
        </record>
        <record id="maize_vat_1" model="account.tax">
            <field name="name"> Add Tax 1%</field>
            <field name="tax_type">other</field>
            <field name="amount">0.01</field>
        </record>
        <record id="vat_parent_5" model="account.tax">
            <field name="name"> VAT 5%(4% + 1%)</field>
            <field name="tax_type">vat</field>
            <field name="amount">0.05</field>
            <field name="include_base_amount">True</field>
            <field name="child_ids" eval="[(6,0,[ref('maize_vat_4'), ref('maize_vat_1')])]"/>
        </record>
        <record id="maize_vat_12" model="account.tax">
            <field name="name"> VAT 12% (2.5% VAT)</field>
            <field name="tax_type">vat</field>
            <field name="amount">0.12</field>
            <field name="include_base_amount">True</field>
            <field name="child_ids" eval="[(6,0,[ref('maize_vat_child_12')])]"/>
        </record>
        <record id="maize_vat_17" model="account.tax">
            <field name="name"> Vat 15% (2.5% VAT)</field>
            <field name="tax_type">vat</field>
            <field name="amount">0.15</field>
            <field name="include_base_amount">True</field>
            <field name="child_ids" eval="[(6,0,[ref('maize_vat_child_15')])]"/>
        </record>
    </data>
</openerp>