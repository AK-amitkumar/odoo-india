<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="maize_no_excise" model="account.tax">
            <field name="name"> No Excise</field>
            <field name="tax_type">ex</field>
            <field name="amount">0.0</field>
        </record>

        <record id="maize_excise_include" model="account.tax">
            <field name="name"> Excise Duty paid</field>
            <field name="tax_type">ex</field>
            <field name="include_base_amount">True</field>
            <field name="amount">0.0</field>
        </record>
        <record id="maize_child_tax2" model="account.tax">
            <field name="name"> Excise 2%</field>
            <field name="tax_type">ex</field>
            <field name="amount">0.02</field>
        </record>
        <record id="maize_child_tax1" model="account.tax">
            <field name="name"> Excise 1%</field>
            <field name="tax_type">ex</field>
            <field name="include_base_amount">True</field>
            <field name="amount">0.01</field>
        </record>
        <record id="maize_excise_12" model="account.tax">
            <field name="name"> Excise 12%</field>
            <field name="tax_type">ex</field>
            <field name="amount">0.12</field>
            <field name="child_ids" eval="[(6,0,[ref('maize_child_tax2'), ref('maize_child_tax1')])]"/>
        </record>
    </data>
</openerp>