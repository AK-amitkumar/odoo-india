        <record id="account_tds_deductee_form2" model="ir.ui.view">
            <field name="name">account.tds.deductee.type.form2</field>
            <field name="model">account.tds.deductee.type</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="TDS Nature">                    
                    <field name="name" />
                    <field name="status" />
                    <field name="residential"/>
                    <field name="active" />
                    <field name="tds_deductee_line" widget="one2many">
                    
                       <form string="TDS Nature">
                            <field name="tds_nature_id"/>
                            
                           <field name='tds_deductee_line_line' widget="one2many_list">
                           
                                 <form string="TDS Nature">
                                            <field name="date_from"/>
                                            
                                            <separator string="Tax Rates"/>
                                            
                                            <field name='tds_rate'/>
                                            <field name='tds_exempt_limit'/>
                                            
                                            <field name='sur_rate'/>
                                            <field name='sur_exempt_limit'/>
                                            
                                            <field name='ed_cess_rate'/>
                                            <field name='ed_exempt_limit'/>
                                            
                                            <field name='sec_ed_cess_rate'/>
                                            <field name='sec_ed_exempt_limit'/>
                                </form>
                                <tree string="TDS Line Line Tree">
                                           <field name="date_from"/>
                                            
                                            <field name='tds_rate'/>
                                            <field name='tds_exempt_limit'/>                                         
                            
                                </tree>

                            </field>
                           
                         </form>
                        <tree string="Deductee Line Tree">
                            <field name="tds_nature_id"/>
                            
                       </tree>
                       
                    </field>
                    
                </form>
            </field>
        </record>
        


