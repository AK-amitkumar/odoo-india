       <!-- Form TDS -->
       
          <record id="account_tds_deductee_form" model="ir.ui.view">
            <field name="name">account.tds</field>
            <field name="model">account.tds</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="TDS">                    
                    <field name="date" />
                    <field name="name" />
                    <field name="tds_nature_id" />
                    <field name="tds_acc_id" />
                    <field name="amount" />
                    <field name="tds_amount" />
                    <field name="tds_payable" />
                    <field name="deductnow" />
                                        
                    <field name="tds_line_id" widget="one2many">
                       <form string="TDS Line">
                            <field name="tds_tax"/>
                            <field name="rate"/>
                            <field name='debit'/>
                            <field name='credit'/>
                            <field name='base'/>
                       </form>                       
                        <tree string="TDS Line Tree">
                            <field name="tds_tax"/>
                            <field name="rate"/>
                            <field name='debit'/>
                            <field name='credit'/>
                            <field name='base'/>
                            
                       </tree>
                    </field>
                    
                </form>
            </field>
        </record>
        
        
        
        
        
          <record id="view_tds_tree" model="ir.ui.view">
            <field name="name">account.tds.tree</field>
            <field name="model">account.tds</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="TDS Entries">                    
                    <field name="date" />
                    <field name="name" />
                    <field name="tds_nature_id" />
                    <field name="tds_acc_id" />
                    <field name="amount" />
                    <field name="tds_amount" />
                    <field name="tds_payable" />                
                    <field name="state" />                    
                    
                </tree>
            </field>
        </record>
        
        
        <record id="view_tds_search" model="ir.ui.view">
            <field name="name">account.tds.search</field>
            <field name="model">account.tds</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Entry Subscription">
                    <group col="8" colspan="4">
                        <filter icon="terp-document-new" string="Draft" domain="[('state','=','draft')]" help="Draft TDS"/>
                        <filter icon="terp-camera_test" string="Confirmed" domain="[('state','=','confirmed')]" help="Confirmed TDS"/>
                        <filter icon="terp-document-new" string="Later" domain="[('state','=','later')]" help="To Be Deducted"/>
                        <filter icon="terp-document-new" string="Pending" domain="[('move_line_id.reconcile_id','=',None)]" help="Pending Payable for Govt"/>
                        <filter icon="terp-document-new" string="PaidTo" domain="[('move_line_id.reconcile_id','!=',None)]" help="Paid to Govt"/>
                        <filter icon="terp-document-new" string="Zero/Lower" domain="[('zero','=',True)]" help="Lower/Zero rate TDS Deductions"/>
                        <separator orientation="vertical"/>
                    
                    </group>
                    <newline/>
                    <group expand="0" string="Group By...">
                        <filter string="NautureOfPayment" icon="terp-folder-orange" domain="[]" context="{'group_by':'tds_nature_id'}"/>
                        <filter string="State" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'state'}"/>
                        <filter string="Account" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'tds_acc_id'}"/>
                    </group>
                </search>
            </field>
        </record>

        
        
        <record id="action_tds_tree" model="ir.actions.act_window">
            <field name="name">TDS Entries</field>
            <field name="res_model">account.tds</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_tds_search"/>
            <field name="view_id" ref="view_tds_tree"/>
            <field name="help">Shows TDS Entries </field>
        </record>
        
        
        <menuitem action="action_tds_tree" id="menu_tds_main" name="TDS Main" parent="account.menu_finance"/>
        
        <menuitem action="action_tds_tree" id="menu_tds_tree" parent="menu_tds_main"/>
        


</data>
</openerp>

