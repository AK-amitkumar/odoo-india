<openerp>
    <data noupdate="1">

    <record  id="group_maize_data" model="res.groups">
        <field name="name">Maize Data</field>
        <field name="category_id" ref="base.module_category_usability"/>
    </record>

    <record id="indent_rule" model="ir.rule">
        <field name="name">Indents</field>
        <field name="model_id" ref="model_indent_indent"/>
        <field name="domain_force">['|',('indentor_id','=',user.id),('employee_id.coach_id','child_of',[emp.id for emp in user.employee_ids])]</field>
        <field name="groups" eval="[(4, ref('stock.group_stock_user'))]"/>
    </record>

