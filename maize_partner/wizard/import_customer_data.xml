<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      <record id="view_import_res_partner_form" model="ir.ui.view">
        <field name="name">import_inward_data_form</field>
        <field name="model">import.res.partner</field>
        <field name="arch" type="xml">
            <form string="Import Inward" version="7.0">
               <group>
                    <separator string="Import Partner Data"/>
               </group>
               <footer>
                    <button name="do_import_partner_data" string="Import" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
               </footer>
           </form>
        </field>
    </record>
    <act_window 
        name="Import Partners"
        res_model="import.res.partner"
        src_model="res.partner"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_partner_import" />

    </data>
</openerp>
