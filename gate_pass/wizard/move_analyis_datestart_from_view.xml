<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
    	<record id="view_move_analysis_datestart_from_form" model="ir.ui.view">
            <field eval="1" name="priority"/>
            <field name="name">move.analysis.datestart.from.form</field>
            <field name="model">move.analysis.datestart.from</field>
            <field name="arch" type="xml">
                <form string="Move Analysis Report" version="7.0">
	                <group>
	                	<group>
	                    	<field name="date_from"/>
	              		</group>
	              		<group>
	              			<field name="date_to"/>
	              		</group>
	                </group>
                    <footer>
                    	<button name="get_data" string="OK" type="object" class="oe_highlight"/> or
                    	<button special="cancel" string="Close" class="oe_link"/>
                    </footer>	
                </form>
            </field>
        </record>
        
        <record id="action_move_analysis_datestart_from" model="ir.actions.act_window">
            <field name="name">Moves Analysis</field>
            <field name="res_model">move.analysis.datestart.from</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_move_analysis_datestart_from_form"/>
            <field name="target">new</field>
        </record>
        
    </data>
</openerp>
