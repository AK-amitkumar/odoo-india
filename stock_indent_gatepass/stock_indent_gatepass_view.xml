<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id='view_gatepass_form' model='ir.ui.view'>
			<field name="name">stock.gatepass.form</field>
			<field name="model">stock.gatepass</field>
			<field name="inherit_id" ref="stock_gatepass.view_gatepass_form"/>
			<field name="arch" type="xml">
				<field name="user_id" position="before">
				    <field name="indent_id" on_change="onchange_indent(indent_id)" attrs="{'invisible': [('approval_required', '=', False)], 'required': 
                                    [('approval_required', '=', True)]}" domain="[('type', '=', 'existing'), ('state', '=', 'inprogress')]" /> 
				</field>
			</field>
		</record>
		
	</data>
</openerp>